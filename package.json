{
  "name": "Slack",
  "version": "1.0.0",
  "description": "Dashboard ui for investing",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss ./css/style.min.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass ./sass/main.scss ./css/style.comp.css",
    "post:css": "postcss --use autoprefixer -b 'last 10 versions' ./css/style.comp.css -o ./css/style.prefix.css",
    "compress:css": "sass ./css/style.prefix.css ./css/style.min.css --style compressed",
    "compile:parcel": "parcel index.html",
    "build:css": "npm-run-all compile:sass post:css compress:css compile:parcel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Suhail-007/dashboard-ui.git"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.8.1",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "sass": "^1.57.1"
  },
  "author": "Suhail-007",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Suhail-007/dashboard-ui/issues"
  },
  "homepage": "https://github.com/Suhail-007/dashboard-ui#readme",
  "keywords": []
}
